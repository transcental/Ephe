            <ui:SliderInt label="Volume" value="50" high-value="100" name="volume-slider" style="width: 511px; align-self: center; align-content: center; background-color: rgb(188, 188, 188); border-top-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; border-bottom-left-radius: 10px;" />
        </ui:VisualElement>
    </ui:VisualElement>
